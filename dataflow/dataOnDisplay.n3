swi:OutputDataset
  a spinx:LoggedGraph ;
  spinx:input [
    a spinx:ConstructGraph ;
    spinx:input
      [ a spinx:InlineDataset ;
        spinx:input swi:InputDataset ;
        spinx:namedInput 
          [ a spinx:NamedGraph ;
            spinx:id :config ;
            spinx:input 
              [ a spinx:SelectGraph ;
                spinx:id :config ;
                spinx:input swi:InputDataset ]
          ] ,
          [ a spinx:NamedGraph ;
            spinx:id :viewerState ;
            spinx:input :viewerState ]
      ] ;
    spinx:config
      [ a spinx:IncludedGraph ;
        spinx:url <viewerPos.sparql> ;
        spinx:syntax query:SPARQL_11 ] ]
.

:inputAndState
  a spinx:InlineDataset ;
  spinx:input swi:InputDataset ;
  spinx:namedInput 
    [ a spinx:NamedGraph ;
      spinx:id :config ;
      spinx:input 
        [ a spinx:SelectGraph ;
          spinx:id :config ;
          spinx:input swi:InputDataset ] ] ;
  spinx:namedInput 
    [ a spinx:NamedGraph ;
      spinx:id :groupData ;
      spinx:input 
        [ a spinx:SelectGraph ;
          spinx:id :groupData ;
          spinx:input swi:InputDataset ] ] ;
  spinx:namedInput 
    [ a spinx:NamedGraph ;
      spinx:id :viewerState ;
      spinx:input :viewerState ] .

